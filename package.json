{
  "name": "zos-dev",
  "displayName": "zOS-Dev",
  "description": "",
  "version": "0.0.8",
  "publisher": "ArnaldoRamirez",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscodeext-activitybar",
          "title": "z/OS Dev",
          "icon": "resources/icons8.png"
        }
      ]
    },
    "views": {
      "vscodeext-activitybar": [
        {
          "id": "vscodeext-view",
          "name": "Herramientas"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "vscodeext-view",
        "contents": "[TSO BUS](command:vscodeext.openTsoBusMultipleWebview)\n [ENDEVOR](command:vscodeext.openEndevorWebview)\n\n[DOWNLOAD DATASET](command:vscodeext.openDownloadDatasetWebview)\n [DESTA](command:vscodeext.openDestaWebview)\n [DELOGUE](command:vscodeext.openDeslogueWebview)"
        
      }
    ],
    "commands": [
      {
        "command": "extension.tuComando",
        "title": "Abrir z/OS Dev"
      },
      {
        "command": "welcome-view-content-sample.hello",
        "title": "Hello World!"
      }
    ]
  },
  "files": [
    "media/**",
    "resources/**"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}