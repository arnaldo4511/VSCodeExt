{
  "name": "vscodealtext",
  "displayName": "VSCodeAltExt",
  "description": "",
  "version": "0.0.1",
  "publisher": "VSCodeAltExt",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscodeext.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscodeext-activitybar",
          "title": "z/OS Dev",
          "icon": "./resources/icon.png"
        }
      ]
    },
    "views": {
      "vscodeext-activitybar": [
        {
          "id": "vscodeext-activitybar.vscodeext-view",
          "name": "Herramientas",
          "content": "Hello World",
          "type": "webview",
          "icon": "./resources/icon.png"
        }
      ]
    }
  },
  "files": [
    "media/**",
    "resources/**"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "author": "Arnaldo Ramirez"
}